FREETYPE_OBJS := $(FREETYPE:.c=.o)
FREETYPE_INC := $(FREETYPE_SRCDIR)/include
$(FREETYPE_SRCDIR)%.o: CFLAGS += -I $(FREETYPE_INC) -I $(FREETYPE_SRCDIR)/builds/unix

FT_CFLAGS := -I $(FREETYPE_INC)
